{
  "name": "eslint-config-wdzeng",
  "version": "0.14.0",
  "description": "wdzeng's ESLint rule presets",
  "author": "hyperbola",
  "license": "MIT",
  "main": "index.mjs",
  "files": [
    "index.mjs",
    "presets/**/*.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wdzeng/eslint-config"
  },
  "scripts": {
    "lint": "eslint --fix",
    "test": "vitest --reporter=verbose",
    "regen": "tests/autofix/regen.sh",
    "clean": "rm -rf tests/autofix/js/backup tests/autofix/ts/backup"
  },
  "dependencies": {
    "@eslint/js": "^9.22.0",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/eslint-plugin": "^1.1.38",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import-x": "^4.9.1",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-path-alias": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.26.1"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.17.24",
    "@typescript-eslint/utils": "^8.26.1",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "eslint": ">=9.20",
    "eslint-plugin-import-x": ">=4.9",
    "eslint-plugin-n": ">=17.11",
    "eslint-plugin-prettier": ">=5.1",
    "eslint-plugin-unicorn": ">=57",
    "prettier": ">=3",
    "typescript": ">=5",
    "typescript-eslint": ">=8.3"
  },
  "engines": {
    "node": "^20.18.3 || ^22.12 || >=v23.1"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^20.12 || >=21.2",
      "onFailer": "error"
    }
  },
  "pnpm": {
    "overrides": {
      "hasown": "npm:@nolyfill/hasown@^1",
      "isarray": "npm:@nolyfill/isarray@^1"
    }
  }
}
