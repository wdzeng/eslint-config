{
  "name": "eslint-config-wdzeng",
  "version": "0.10.0",
  "description": "wdzeng's ESLint rule presets",
  "author": "hyperbola",
  "license": "MIT",
  "main": "index.mjs",
  "files": [
    "index.mjs",
    "presets/**/*.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wdzeng/eslint-config"
  },
  "scripts": {
    "lint": "eslint --fix",
    "test": "cp -r tests/js/in tests/js/out && cp -r tests/ts/in tests/ts/out && ava; pnpm clean",
    "regen": "tests/regen.sh",
    "clean": "rm -rf tests/js/out tests/ts/out"
  },
  "dependencies": {
    "@eslint/js": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.10.0",
    "typescript-eslint": "^8.8.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.16.10",
    "ava": "^6.1.3",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "typescript": "~5.5.4",
    "typescript-eslint": "8.8.0"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "eslint-plugin-import": ">=2.31",
    "eslint-plugin-n": ">=17",
    "eslint-plugin-prettier": "^5.1",
    "eslint-plugin-unicorn": ">= 56",
    "prettier": ">=3",
    "typescript": ">=5 <5.6",
    "typescript-eslint": ">=8"
  },
  "engines": {
    "node": ">=20"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^20.12 || ^21.2 || >=22",
      "onFailer": "error"
    }
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "string.prototype.trimend": "npm:@nolyfill/string.prototype.trimend@^1"
    }
  }
}
