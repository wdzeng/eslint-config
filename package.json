{
  "name": "eslint-config-wdzeng",
  "version": "0.12.0",
  "description": "wdzeng's ESLint rule presets",
  "author": "hyperbola",
  "license": "MIT",
  "main": "index.mjs",
  "files": [
    "index.mjs",
    "presets/**/*.mjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wdzeng/eslint-config"
  },
  "scripts": {
    "lint": "eslint --fix",
    "test": "vitest --reporter=verbose",
    "regen": "tests/autofix/regen.sh",
    "clean": "rm -rf tests/autofix/js/backup tests/autofix/ts/backup"
  },
  "dependencies": {
    "@eslint/js": "^9.12.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-n": "^17.11.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.11.0",
    "json5": "^2.2.3",
    "typescript-eslint": "^8.10.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.16.12",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "8.10.0",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "eslint-plugin-import-x": ">=4",
    "eslint-plugin-n": ">=17.11",
    "eslint-plugin-prettier": ">=5.1",
    "eslint-plugin-unicorn": ">=56",
    "prettier": ">=3",
    "typescript": ">=5",
    "typescript-eslint": ">=8"
  },
  "engines": {
    "node": ">=20"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^20.12 || ^21.2 || >=22",
      "onFailer": "error"
    }
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "string.prototype.trimend": "npm:@nolyfill/string.prototype.trimend@^1"
    }
  }
}
