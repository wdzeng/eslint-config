{
  "name": "eslint-config-wdzeng",
  "version": "0.10.0",
  "description": "wdzeng's ESLint rule presets",
  "author": "hyperbola",
  "license": "MIT",
  "main": "index.js",
  "packageManager": "pnpm@^8.9.0",
  "files": [
    "index.js",
    "typescript.js",
    "presets/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wdzeng/eslint-config"
  },
  "scripts": {
    "lint": "eslint --fix",
    "test": "cp -r tests/js/in tests/js/out && cp -r tests/ts/in tests/ts/out && ava; pnpm clean",
    "regen": "tests/regen.sh",
    "clean": "rm -rf tests/js/out tests/ts/out"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^54.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.6",
    "ava": "^6.1.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "prettier": "^3.3.2"
  },
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "has": "npm:@nolyfill/has@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.groupby": "npm:@nolyfill/object.groupby@latest",
      "object.values": "npm:@nolyfill/object.values@latest",
      "hasown": "npm:@nolyfill/hasown@latest"
    }
  }
}
