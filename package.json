{
  "name": "eslint-config-wdzeng",
  "version": "0.5.0",
  "description": "wdzeng's ESLint rule presets",
  "author": "hyperbola",
  "license": "MIT",
  "main": "index.js",
  "packageManager": "pnpm@^8.9.0",
  "files": [
    "index.js",
    "typescript.js",
    "presets/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wdzeng/eslint-config"
  },
  "scripts": {
    "lint": "eslint --fix",
    "regen-testcases": "pnpm regen-testcases:js; pnpm regen-testcases:ts",
    "regen-testcases:js": "rm -rf tests/js/out && cp -r tests/js/in tests/js/out && eslint --config tests/js/.eslintrc.test.cjs --no-ignore --fix tests/js/out >/dev/null",
    "regen-testcases:ts": "rm -rf tests/ts/out && cp -Lr tests/ts/in tests/ts/out && REGEN_TESTCASES=1 eslint --config tests/ts/.eslintrc.test.cjs --no-ignore --fix tests/ts/out >/dev/null",
    "test": "cp -r tests/js/in tests/js/tmp && cp -Lr tests/ts/in tests/ts/tmp && ava; pnpm clean",
    "clean": "rm -rf tests/js/tmp tests/ts/tmp"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unicorn": "^48.0.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.3",
    "@types/node": "^20.8.4",
    "ava": "^5.3.1",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "engines" : {
    "node" : ">=20"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "has": "npm:@nolyfill/has@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.groupby": "npm:@nolyfill/object.groupby@latest",
      "object.values": "npm:@nolyfill/object.values@latest"
    }
  }
}
